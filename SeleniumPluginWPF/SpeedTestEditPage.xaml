    <Page x:Class="SeleniumPluginWPF.SpeedTestEditPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SeleniumPluginWPF"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="SpeedTestEditPage">

    <Grid>
        <StackPanel>
            <Label x:Name="lbl1" Content="Label" />
            <TextBox x:Name="URLTextBox" Tag="URL" />
            <Button x:Name="Button1" Click="Button1_Click" Content="Button" Width="50" Margin="5"/>
            <ComboBox x:Name="SpeedComboBox" Tag="Speed" />
        </StackPanel>


    </Grid>
</Page>
